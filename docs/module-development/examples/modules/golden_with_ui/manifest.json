{
  "schemaVersion": "1.0",
  "name": "golden-with-ui",
  "version": "0.1.0",
  "type": "with-ui",
  "computeType": "fargate",
  "description": "Reference UI-enabled module covering the SDK golden path.",
  "execution": {
    "entrypoint": "backend.main:GoldenWithUIModule",
    "supports": [
      "interactive"
    ],
    "defaultMode": "interactive"
  },
  "author": {
    "name": "Alithea Bio",
    "email": "modules@alithea.bio",
    "organization": "Reference"
  },
  "ui": {
    "framework": "react",
    "buildCommand": "npm run build",
    "distPath": "frontend/dist"
  },
  "inputs": {
    "type": "object",
    "title": "Module Inputs",
    "required": ["param1"],
    "properties": {
      "param1": {
        "type": "string",
        "title": "Required text",
        "description": "Value echoed to the UI"
      },
      "param2": {
        "type": "string",
        "title": "Optional text",
        "description": "Optional secondary value",
        "default": "default_value"
      }
    }
  },
  "outputs": {
    "results": {
      "type": "object",
      "description": "UI tables, insights, and context metadata"
    },
    "summary": {
      "type": "object",
      "description": "High-level summary for display"
    }
  },
  "resources": {
    "memory": 512,
    "timeout": 180,
    "environment": {
      "LOG_LEVEL": "INFO"
    }
  },
  "permissions": {
    "database": ["read"],
    "storage": true,
    "network": []
  },
  "metadata": {
    "exposedParameters": [],
    "auditLog": true
  },
  "pricing": {
    "tiers": [
      {
        "tier": "developer",
        "model": "usage-based",
        "amountAct": 0.15
      }
    ]
  }
}
