{
  "schemaVersion": "1.0",
  "name": "golden-with-ui",
  "version": "0.1.0",
  "type": "with-ui",
  "computeType": "docker",
  "description": "Reference UI-enabled module covering the SDK golden path.",
  "execution": {
    "entrypoint": "backend.main:GoldenWithUIModule",
    "supports": ["interactive"],
    "defaultMode": "interactive",
    "contextContract": "module-run-context-v1"
  },
  "author": {
    "name": "Alithea Bio",
    "email": "modules@alithea.bio",
    "organization": "Reference"
  },
  "runtime": "python3.11",
  "inputs": {
    "type": "object",
    "required": ["param1"],
    "properties": {
      "param1": {
        "type": "string",
        "title": "Required text",
        "description": "Value echoed to the UI"
      },
      "param2": {
        "type": "string",
        "title": "Optional text",
        "default": "default_value"
      }
    }
  },
  "outputs": {
    "results": {
      "type": "object",
      "description": "UI tables, insights, and context metadata"
    }
  },
  "resources": {
    "memory": 512,
    "timeout": 300
  },
  "permissions": {
    "database": ["read"],
    "storage": true,
    "network": []
  },
  "ui": {
    "bucket": "TODO: set via publish defaults",
    "bundleKey": "modules/golden-with-ui/0.1.0/bundle.js"
  },
  "pricing": {
    "tiers": [
      {
        "tier": "developer",
        "model": "usage-based",
        "amountAct": 0.15
      }
    ]
  }
}
